<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.quarkstar.freedom.NearbySearchFragment"
             android:paddingTop="?android:attr/actionBarSize"
             tools:layout_editor_absoluteX="0dp"
             tools:layout_editor_absoluteY="80dp">

    <android.support.constraint.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/activity_nearby_search"
            android:layout_width="411dp"
            android:layout_height="601dp"
            tools:context="com.quarkstar.freedom.NearbySearchActivity"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp">

        <SearchView
                android:layout_width="0dp"
                android:layout_height="48dp"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="16dp"
                android:id="@+id/searchView2"
                app:layout_constraintLeft_toLeftOf="@+id/activity_nearby_search"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintTop_toTopOf="@+id/activity_nearby_search"
                android:layout_marginTop="16dp"
                tools:layout_constraintTop_creator="1"
                app:layout_constraintRight_toRightOf="@+id/activity_nearby_search"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                tools:layout_constraintRight_creator="1"/>

        <ListView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="144dp"
                android:id="@+id/listView"
                app:layout_constraintLeft_toLeftOf="@+id/activity_nearby_search"
                android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp"
                tools:layout_constraintLeft_creator="1"
                app:layout_constraintTop_toBottomOf="@+id/searchView2"
                android:layout_marginTop="80dp"
                tools:layout_constraintTop_creator="1"
                app:layout_constraintRight_toRightOf="@+id/activity_nearby_search"
                android:layout_marginRight="8dp"
                android:layout_marginEnd="8dp"
                tools:layout_constraintRight_creator="1"
                app:layout_constraintHorizontal_bias="0.62"
                android:textAlignment="center"/>

        <EditText
                android:layout_width="395dp"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:text="Seacrh Near By Restaurants"
                android:ems="10"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="79dp"
                android:id="@+id/editText"
                app:layout_constraintLeft_toLeftOf="@+id/activity_nearby_search"
                tools:layout_constraintLeft_creator="0"
                app:layout_constraintRight_toRightOf="@+id/activity_nearby_search"
                tools:layout_constraintRight_creator="0"
                app:layout_constraintBottom_toTopOf="@+id/listView"
                android:layout_marginBottom="24dp"
                tools:layout_constraintBottom_creator="0"/>
    </android.support.constraint.ConstraintLayout>


</FrameLayout>
